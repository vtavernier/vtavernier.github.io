---
import '@fontsource/inter/variable.css';

// Nunito
// Dosis
---

<style is:inline is:global>
  :root {
    --aw-font-sans: 'InterVariable';
    --aw-font-serif: var(--aw-font-sans);
    --aw-font-heading: var(--aw-font-sans);

    --aw-color-primary: rgb(89 30 100);
    --aw-color-secondary: rgb(94 30 100);
    --aw-color-accent: rgb(100 59 40);
    --aw-color-text-page: rgb(39 17 39);
    --aw-color-text-muted: rgb(97 75 99);
    --aw-color-bg-page: rgb(255 255 255);
  }
</style>
